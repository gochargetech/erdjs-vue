{
    "name": "erdjs-vue",
    "version": "0.9.20",
    "private": false,
    "description": "A Vue.js library that holds the core functional logic of a dapp on the MultiversX",
    "keywords": [
        "vue",
        "vuejs",
        "vue.js",
        "erdjs",
        "erdjs-vue",
        "multiversx",
        "elrond",
        "blockchain",
        "dapp",
        "smart-contract",
        "SC",
        "transaction",
        "bootstrap",
        "gocharge",
        "gochargetech",
        "gocharge.tech"
    ],
    "author": {
        "name": "goCharge.tech",
        "email": "contact@goCharge.tech",
        "url": "https://gocharge.tech/"
    },
    "type": "module",
    "types": "erdjs-vue/main.d.ts",
    "main": "dist/erdjs-vue.js",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "build-watch": "vite build --watch",
        "build-prod": "type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "peerDependencies": {
        "vue": "^3"
    },
    "optionalDependencies": {
        "bootstrap": "^5.2.3",
        "platform": "1.3.6",
        "qrcode": "1.5.0",
        "swr": "1.1.2",
        "vue-router": "^4.1.6"
    },
    "dependencies": {
        "@multiversx/sdk-core": "12.18.0",
        "@multiversx/sdk-extension-provider": "3.0.0",
        "@multiversx/sdk-hw-provider": "6.4.0",
        "@multiversx/sdk-network-providers": "2.2.0",
        "@multiversx/sdk-wallet": "4.3.0",
        "@multiversx/sdk-wallet-connect-provider": "4.1.0",
        "@multiversx/sdk-web-wallet-provider": "3.2.0",
        "anchorme": "2.1.2",
        "axios": "^1.2.0",
        "pinia": "^2.0.27",
        "pinia-plugin-persistedstate": "^3.0.1",
        "socket.io-client": "4.5.1"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/jsdom": "^20.0.1",
        "@types/lodash": "^4.14.181",
        "@types/node": "^18.11.9",
        "@types/platform": "^1.3.4",
        "@types/qrcode": "1.4.2",
        "@types/qs": "6.9.7",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vitejs/plugin-vue-jsx": "^2.1.1",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.2.4",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "sass": "^1.56.1",
        "typescript": "~4.7.4",
        "vite": "^3.2.4",
        "vitest": "^0.25.3",
        "vue-tsc": "^1.0.9"
    },
    "bugs": {
        "url": "https://github.com/gochargetech/erdjs-vue/issues",
        "email": "contact@gocharge.tech"
    }
}